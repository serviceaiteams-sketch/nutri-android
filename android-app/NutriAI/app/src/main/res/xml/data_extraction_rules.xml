<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <!-- Exclude authentication data -->
        <exclude domain="sharedpref" path="auth_prefs.xml"/>
        <exclude domain="file" path="auth/"/>
        
        <!-- Exclude cache -->
        <exclude domain="cache" />
        
        <!-- Exclude temporary files -->
        <exclude domain="file" path="temp/"/>
    </cloud-backup>
    
    <device-transfer>
        <!-- Include all data for device transfer except authentication -->
        <exclude domain="sharedpref" path="auth_prefs.xml"/>
    </device-transfer>
</data-extraction-rules>

